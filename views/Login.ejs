<!DOCTYPE html>
<html>

<head>
    <style>
        div {
            width: 100px;
            height: 100px;
            background-color: red;
            position: absolute;
            animation-duration: 2s;
        }
    </style>
</head>

<body>

    <p><b>Note:</b> This example does not work in Internet Explorer 9 and earlier versions.</p>

    <div id="transf"></div>

    <button> click</button>
    <script>
        function clicked(nameAnimation) {
            var a = document.getElementById("transf");
            a.style.animationName = nameAnimation;
        }
    </script>
    <script>
        var tag = document.getElementById("transf");

        dragElement(tag);




        function dragElement(elmnt) {
            var pos1 = 0,
                pos2 = 0,
                pos3 = 0,
                pos4 = 0;

            /* otherwise, move the DIV from anywhere inside the DIV:*/
            elmnt.onmousedown = dragMouseDown;


            function dragMouseDown(e) {
                e = e || window.event;
                e.preventDefault();
                // get the mouse cursor position at startup:
                pos3 = e.clientX;
                pos4 = e.clientY;
                document.onmouseup = closeDragElement;
                // call a function whenever the cursor moves:
                document.onmousemove = elementDrag;
            }

            function elementDrag(e) {
                e = e || window.event;
                e.preventDefault();
                // calculate the new cursor position:
                pos1 = pos3 - e.clientX;
                pos2 = pos4 - e.clientY;
                pos3 = e.clientX;
                pos4 = e.clientY;
                // set the element's new position:
                elmnt.style.top = (elmnt.offsetTop - pos2) + "px";
                elmnt.style.left = (elmnt.offsetLeft - pos1) + "px";
                // dragCoinToBgFrame(elmnt.offsetLeft, elmnt.offsetTop)
            }

            function closeDragElement() {
                /* stop moving when mouse button is released:*/
                document.onmouseup = null;
                document.onmousemove = null;
                // addCoinIntoFrameBox(elmnt.offsetLeft, elmnt.offsetTop, id);
                console.log(elmnt.offsetTop + 1);
                console.log(elmnt.offsetLeft + 1);
                myMove()
            }

            function setPositionElement(element) {
                // while (element.offsetTop > 0 || element.offsetLeft > 0) {
                //     element.offsetLeft -= 10;
                //     element.offsetTop -= 10;
                // }

            }

            function myMove() {
                var elem = document.getElementById("transf");
                var pos_top = elem.offsetTop;
                var pos_left = elem.offsetLeft;
                var value_top = Math.abs(350 - pos_top);
                var value_left = Math.abs(350 - pos_left);
                var id = setInterval(frame, 5);

                function frame() {
                    if (pos_top == 350 && pos_left == 350) {
                        clearInterval(id);
                    } else {
                        if (pos_top < 350) {
                            pos_top += value_top / 100;
                        }
                        if (pos_top > 350) {
                            pos_top -= value_top / 100;
                        }
                        if (pos_left < 350) {
                            pos_left += value_left / 100;
                        }
                        if (pos_left > 350) {
                            pos_left -= value_left / 100;
                        }
                        if (pos_left > 350 - value_left / 100 && pos_left < 350 + value_left / 100) pos_left = 350;
                        if (pos_top > 350 - value_top / 100 && pos_top < 350 + value_top / 100) pos_top = 350;
                        elem.style.top = pos_top + "px";
                        elem.style.left = pos_left + "px";
                    }
                }
            }
        }
    </script>
</body>

</html>